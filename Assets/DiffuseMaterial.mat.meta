using UnityEngine;

public class MaterialChanger : MonoBehaviour
{
    [SerializeField] private Material newMaterial;

    void Start()
    {
        var renderer = GetComponent<Renderer>();
        if (renderer != null && newMaterial != null)
        {
            renderer.material = newMaterial; // Assign at runtime
        }
    }
}
